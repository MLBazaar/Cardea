{
    "name": "keras.preprocessing.sequence.pad_sequences",
    "author": "Carles Sala <carles@pythiac.com>",
    "documentation": "https://keras.io/preprocessing/sequence/#pad_sequences",
    "description": "Pads sequences to the same length.",
    "classifiers": {
        "type": "preprocessor",
        "subtype": "transformer"
    },
    "modalities": [],
    "primitive": "keras.preprocessing.sequence.pad_sequences",
    "produce": {
        "args": [
            {
                "name": "X",
                "keyword": "sequences",
                "type": "ndarray"
            }
        ],
        "output": [
            {
                "name": "X",
                "type": "ndarray"
            }
        ]
    },
    "hyperparameters": {
        "fixed": {
            "dtype": {
                "type": "str",
                "default": "int32"
            },
            "value": {
                "type": "float",
                "default": 0.0
            }
        },
        "tunable": {
            "maxlen": {
                "type": "int",
                "default": 100,
                "values": [1, 10000]
            },
            "padding": {
                "type": "str",
                "default": "pre",
                "values": ["pre", "post"]
            },
            "truncating": {
                "type": "str",
                "default": "pre",
                "values": ["pre", "post"]
            }
        }
    }
}
